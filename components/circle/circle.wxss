/* components/circle/circle.wxss */
.container{
  margin: 70rpx;
  padding: 0;
  padding-left: 40rpx;
  border-radius: 30rpx;
  padding-top: 60rpx;
  width: 600rpx;
  height: 400rpx;
  background-color: rgb(234, 242, 255);
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}
.content{
  text-align: right;
  display: inline-block;
  font-size: 45rpx;
  font-weight: 550;
  height: 300rpx;
  line-height: 100rpx;
  margin-right: 30rpx;
}
/* 圆形进度条/circle.wxss */
.circle{
  height: 300rpx;
  width: 300rpx;
  background-color: #29b0f7;
  border-radius: 50%;
  z-index: 1;
}
.halfgrey2{
  content: "";
  position: absolute;
  display: inline-block;
  height: 150rpx;
  width: 300rpx;
  background-color: #9fd3f4;
  border-radius: 150rpx 150rpx 0 0;
  transform-origin: 50% 100%;
  transform: rotate(-90deg);
}
.halfgrey1{
  content: "";
  position: absolute;
  display: inline-block;
  height: 300rpx;
  width: 150rpx;
  background-color: #9fd3f4;
  border-radius: 0 150rpx 150rpx 0;
  transform-origin: 100% 50%;
}
.halfblue{
  content: "";
  position: absolute;
  display: inline-block;
  background-color: #29b0f7;
  height: 300rpx;
  width: 150rpx;
  border-radius: 0 150rpx 150rpx 0;
  transform-origin: 100% 50%;
}
.innerbox{
  position: relative;
  left: 50%;
  top: 50%;
  height: 200rpx;
  width: 200rpx;
  background-color:#eaf2ff;
  border-radius: 50%;
  z-index: 5;
  transform: translateX(-50%) translateY(-50%);
}
.progress{
  color: rgb(87, 150, 231);
  font-size: 40rpx;
}
.icon-xingxing{
  color: rgb(146, 189, 255);
  position: absolute;
  top: 35%;
  left: 50%;
  transform: translate(-50%,-50%) scale(4);
}
@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff2;charset=utf-8;base64,') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

