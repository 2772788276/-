/* 方向页面各组详情页样式 */
.navigator{
  background-color: rgb(87, 150, 231);
  padding-top: 50rpx;
  height: 200rpx;
  color: white;
  font-size: 50rpx;
  font-weight: 600;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.icon-xiangzuo{
  margin-right: 40rpx;
  margin-left: 40rpx;
  font-size: 60rpx;
}
.directionInImage {
  width: 100%;
  height: 420rpx;
}
.directionInColor {
  background-color: rgb(87, 150, 231);
  width: 100%;
  height: 100%;
  padding: 18rpx 0;
}
.directionMain {
  background-color: #eee;
  border-radius: 60rpx;
  margin:0 40rpx;
  padding: 24rpx 60rpx 40rpx;
}
.groupImage {
  box-sizing: border-box;
  padding: 50rpx;
  padding-bottom: 24rpx;
  width: 100%;
  height: 420rpx;
}
.directionMainTitle {
 text-align: center;
}
.groupTitle1 {
  padding: 0 24rpx;
  color: rgb(98, 99, 100);
  font-weight: bold;
  font-size: 18px;
}
.icon-shanshuo,
.directionHr {
  color: rgb(254, 250, 131);
  font-weight: bold;
  font-size: 18px;
}
.directionHr {
 padding: 0 12rpx;
}
.icon-yuandian {
  font-weight: bolder;
  font-size: 17px;
  color: rgb(111, 175, 236);
}
.groupTitle2 {
  font-weight: bold;
  font-size: 17px;
  color: rgb(51, 51, 51);
}
.groupTextTitle{
  padding: 26rpx 0;
}
.groupText {
  font-size: 14px;
  color: rgb(16, 16, 16);
  line-height: 24px;
  text-align: left;
  /* padding: 26rpx 0; */
}
.groupText1 {
  text-indent:2em;
}

