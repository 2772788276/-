.star1{
  transform: scale(6) rotate(45deg);
  transform-origin: 10rpx;
  position: absolute;
  right: 90rpx;
  top: 330rpx;
  animation: star1 1s 1 forwards,rotate1 1.5s 1s infinite;
}
.star2{
  transform: scale(4) rotate(-45deg);
  position: absolute;
  left: 80rpx;
  top: 650rpx;
  animation: star 1s 0.5s 1 forwards,rotate 1.5s 1.5s infinite ;
  opacity: 0;
}
.star3{
  transform: scale(6) rotate(-45deg);
  position: absolute;
  bottom: 230rpx;
  right: 250rpx;
  animation: star3 1s 1 forwards,rotate3 1.5s 1s infinite;
  transform-origin: center;
}
.star4{
  transform: scale(4) rotate(45deg);
  position: absolute;
  bottom: 120rpx;
  right: 60rpx;
  opacity: 0;
  animation: star4 1s 0.5s 1 forwards;
}
.star5{
  transform: scale(4) rotate(-45deg);
  position: absolute;
  top: 190rpx;
  right: 150rpx;
  animation: star 1s 1 forwards,rotate 1.5s 1.5s infinite;
}
@keyframes star3{
  0%{
    transform: scale(0) rotate(-45deg);
    opacity: 0;
  }
  80%{
    transform: scale(7) rotate(-45deg);
    opacity: 1;
  }
  100%{
    transform: scale(6) rotate(-45deg);
    opacity: 1;
  }
}
@keyframes star1{
  0%{
    transform: scale(0) rotate(45deg);
    opacity: 0;
  }
  80%{
    transform: scale(7) rotate(45deg);
    opacity: 1;
  }
  100%{
    transform: scale(6) rotate(45deg);
    opacity: 1;
  }
}
@keyframes star{
  0%{
    transform: scale(0) rotate(-45deg);
    opacity: 0;
  }
  80%{
    transform: scale(5) rotate(-45deg);
    opacity: 1;
  }
  100%{
    transform: scale(4) rotate(-45deg);
    opacity: 1;
  }
}
@keyframes star4{
  0%{
    transform: scale(0) rotate(45deg);
    opacity: 0;
  }
  80%{
    transform: scale(5) rotate(45deg);
    opacity: 1;
  }
  100%{
    transform: scale(4) rotate(45deg);
    opacity: 1;
  }
}
@keyframes rotate{
  0%{
    transform: scale(4) rotate(-30deg);
  }
  50%{
    transform: scale(4) rotate(-60deg);
  }
  100%{
    transform: scale(4) rotate(-30deg);
  }
}
@keyframes rotate1{
  0%{
    transform: scale(6) rotate(30deg);
  }
  50%{
    transform: scale(6) rotate(60deg);
  }
  100%{
    transform: scale(6) rotate(30deg);
  }
}
@keyframes rotate3{
  0%{
    transform: scale(6) rotate(-30deg);
  }
  50%{
    transform: scale(6) rotate(-60deg);
  }
  100%{
    transform: scale(6) rotate(-30deg);
  }
}

