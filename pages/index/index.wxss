.tabbar {
  border-top: gray solid 1rpx;
  z-index: 99;
  width: 100%;
  /* height: 100rpx; */
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  background: white;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 60rpx;
  /* iphone安全距离 */
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

.tabbar .item {
  flex: 1;
  color: black;
  height: 100rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  position: relative;
  z-index: 10;

}
.item .icontext {
  position: absolute;
  width: 100%;
  bottom: 15rpx;
  text-align: center;
  font-size: 22rpx;
  opacity: 0;
  transition: all .8s;
  transform: scale(0.8);
  width: 100%;
}

.item.active .icontext {
  opacity: 1;
  transform: scale(1);
}


.item.active .icon {
  color: #3561f5;
  transform: translateY(-30rpx);
}

.item .icon {
  font-size: 50rpx !important;
  text-align: center;
  transition: all .8s;
}

page {
  width: 100%;
